<div class="container">
  <h1 class="mt-4">Add a new venue</h1>
  <%= simple_form_for(@venue) do |f|%>
    <%= f.input :name %>
    <%= f.input :description %>
    <%= f.input :location %>
    <%= f.input :capacity %>
    <%= f.input :price_per_hour %>
    <%= f.submit %>
  <% end %>
  <p class="mt-3"><%= link_to "Back to venues", venues_path %></p>
</div>
